{
  "name": "alfresco-process-workspace",
  "description": "Alfresco Process Workspace",
  "version": "1.3.2",
  "author": "Alfresco Software, Ltd.",
  "commit": "b6f2e20c4874a88863691778c8e52e6c0ab84a17",
  "scripts": {
    "ng": "ng",
    "start": "rimraf dist && ng serve --host 0.0.0.0 --project dist --open --aot=false --base-href",
    "start:dev": "npm run style:dev & npm run copy:dev & npm run clean-ng2-component-angular && rimraf dist && ng serve --host 0.0.0.0 --project dev --proxy-config proxy.conf.js --open",
    "start:dist": "rimraf dist && ng serve --host 0.0.0.0 --disable-host-check --aot=true --base-href",
    "build": "npm run validate-config && rimraf dist && node --max_old_space_size=4098 ./node_modules/@angular/cli/bin/ng build --prod --aot=true --build-optimizer=true --project dist --base-href / --deploy-url",
    "build:dev": "npm run validate-config && rimraf dist && ng build --project dev",
    "style:dev": "npm run webpack -- --config  config/webpack.style.js --progress --profile --bail --watch",
    "copy:dev": "node ./config/dev-copy-watch.js",
    "test": "ng test --watch=false",
    "lint": "ng lint && npm run rx-rules",
    "rx-rules": "./node_modules/.bin/tslint -c rx-tslint.json -p tsconfig.json",
    "lint-e2e": "./node_modules/.bin/tslint --fix -p ./e2e/tsconfig.e2e.json -c tslint.json",
    "replace-appconfig": "node appConfigReplace.js",
    "coverage": "http-server -c-1 ./coverage -p 4344",
    "e2e": "npm run lint-e2e && ng e2e --port=4200",
    "validate-config": "ajv validate -s ./config/schema/schema.json -d ./src/app.config.json --errors=text --verbose",
    "clean": "npm run clean-build  && rimraf dist node_modules typings dist",
    "clean-ng2-component-angular": "rimraf ../alfresco-ng2-components/lib/node_modules/@angular",
    "clean-build": "rimraf 'app/{,**/}**.js' 'app/{,**/}**.js.map' 'app/{,**/}**.d.ts'",
    "clean-lock": "rimraf package-lock.json",
    "webpack": "node node_modules/webpack/bin/webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alfresco/alfresco-process-workspace-app.git"
  },
  "bugs": {
    "url": "https://issues.alfresco.com/jira/projects/DW"
  },
  "license": "SEE LICENSE IN license.txt",
  "keywords": [
    "ng2",
    "angular",
    "angular2",
    "aps2",
    "alfresco"
  ],
